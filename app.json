{
  "expo": {
    "name": "LifeChef",
    "slug": "lifechef-mip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lifechef-icon.jpeg",
    "splash": {
      "image": "./assets/images/lifechef-icon.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "lifechefmip",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.lifechefmip"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.anonymous.lifechefmip"
    }
  }
}
